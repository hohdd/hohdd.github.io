---
layout: compress
draft: true
---
{% assign serial = site.collections | where: 'isSerial', true | where: 'show', true | where: 'label', page.collection | first %}
{% assign docs = serial.docs | where: 'draft', false %}
[
  {% for doc in docs %}
    {
      "title"    : "{{ doc.title | escape }}",
      "url"      : "{{ doc.baseurl }}{{ doc.url }}",
      "date"     : "{{ doc.date | date: '%F' }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]